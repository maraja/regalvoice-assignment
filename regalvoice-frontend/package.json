{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:linux": "rm -rf dist && cross-env NODE_ENV=production parcel build src/index.html",
    "build": "del /f dist && cross-env NODE_ENV=production parcel build src/index.html",
    "deploy:linux": "bash ./deploy.sh",
    "deploy": "deploy.windows.bat",
    "predeploy": "cross-env NODE_ENV=production yarn build",
    "watch": "cross-env NODE_ENV=development parcel --port=7001 src/index.html"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "babel-plugin-import": "^1.13.0",
    "less": "^3.11.1",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "@types/react": "^16.8.0",
    "antd": "^4.6.2",
    "apollo-cache-inmemory": "~1.6.5",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "~1.5.16",
    "axios": "^0.19.2",
    "config": "^3.3.1",
    "cross-env": "^7.0.2",
    "got": "^11.5.1",
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "graphql-tag": "~2.10.1",
    "http2": "^3.3.7",
    "react": "^16.13.1",
    "react-apollo": "~3.1.3",
    "react-dom": ">= 16.8.0",
    "react-hook-form": "~3.29.4",
    "react-redux": "~7.1.3",
    "react-router-dom": "^5.2.0",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "styled-components": "~4.4.1",
    "yup": "~0.28.0"
  },
  "alias": {
    "#root": "./src"
  }
}
